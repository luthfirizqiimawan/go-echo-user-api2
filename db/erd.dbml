// DBML for dbdiagram.io
// Current schema: users
// Suggested auth extension included as UsersExtended

Table users {
  id integer [pk, increment]
  name varchar
  age integer
}

Table users_extended {
  id integer [pk, increment]
  name varchar
  email varchar [unique]
  password_hash varchar
  age integer
  created_at datetime
  updated_at datetime
}

Ref: users_extended.id > users.id // logical mapping if migrated
